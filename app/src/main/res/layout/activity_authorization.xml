<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/RegistrImV1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/txtsign1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.147" />

    <ImageView
        android:id="@+id/RegistrImV2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/fuguri"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/RegistrLog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/logini"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingLeft="55dp"
        android:paddingTop="40dp"
        android:password="false"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.323"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/RegistrPas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/registrere"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingLeft="55dp"
        android:paddingTop="40dp"
        android:password="true"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.463"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/RegistrBut1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/butsign"
        android:onClick="AButton1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.683"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/RegistrImV3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/figurerere"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.772" />

    <ImageView
        android:id="@+id/RegistrImV5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/txtnew1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/RegistrImV2"
        app:layout_constraintVertical_bias="0.856" />

    <ImageButton
        android:id="@+id/RegistrBut2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/txtnew2"
        android:onClick="AButton2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/RegistrImV2"
        app:layout_constraintVertical_bias="0.894"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>